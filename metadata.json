    "sales_orders_data": {
       "description": "Details of sales orders placed by customers.",
       "columns": [
          {
             "name": "SalesOrderID",
             "type": "object",
             "description": "Unique identifier for the sales order."
          },
          {
             "name": "CustomerID",
             "type": "object",
             "description": "Foreign key referencing customers_data CustomerID."
          },
          {
             "name": "ProductID",
             "type": "object",
             "description": "Foreign key referencing products_data ProductID."
          },
          {
             "name": "OrderDate",
             "type": "datetime64[ns]",
             "description": "Date and time when the order was placed."
          },
          {
             "name": "Quantity",
             "type": "int32",
             "description": "Number of product units ordered."
          },
          {
             "name": "ShipmentDate",
             "type": "datetime64[ns]",
             "description": "Date and time when the order was shipped."
          },
          {
             "name": "OrderStatus",
             "type": "object",
             "description": "Current status of the sales order (e.g., 'In Process', 'Shipped', etc.)."
          }
       ],
       "sample_row": {
